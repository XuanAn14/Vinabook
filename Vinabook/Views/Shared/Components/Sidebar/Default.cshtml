@{
}

@model IEnumerable<Vinabook.Models.DanhMuc>
<div class="category-left">
    <h3 class="category-left__title">Danh mục</h3>

    <ul class="category-left__list">
        @foreach (var item in Model)
        {
            <li class="category-left__item">
            <a href="/DanhMuc" class="category-left__link">
                <span>
                    @item.Ten
                </span>

                <i class="bi bi-chevron-compact-right"></i>
            </a>
        </li>
        }
    </ul>

</div>